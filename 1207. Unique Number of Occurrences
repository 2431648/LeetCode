class Solution {
public:
    bool uniqueOccurrences(vector<int>& arr) {
        unordered_map<int,int> freq;
        for (int n : arr) freq[n]++;
        unordered_set<int> s;
        for (auto& [k,v] : freq) {
            if (!s.insert(v).second) return false;
        }
        return true;
    }
};
