/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     struct ListNode *next;
 * };
 */
int pairSum(struct ListNode* head) {
    int n = 0;
    struct ListNode* curr = head;
    while (curr) {
        n++;
        curr = curr->next;
    }
    int* arr = (int*) malloc(sizeof(int) * n);
    curr = head;
    for (int i = 0; i < n; i++) {
        arr[i] = curr->val;
        curr = curr->next;
    }
    int maxSum = 0;
    for (int i = 0; i < n / 2; i++) {
        int twinSum = arr[i] + arr[n - 1 - i];
        if (twinSum > maxSum) maxSum = twinSum;
    }
    free(arr);
    return maxSum;
}
